# Keycloak Authentication Configuration for Backend
# Copy this file to .env and fill in the values

# Keycloak Server URL
KEYCLOAK_URL=http://localhost:8081

# Keycloak Realm
KEYCLOAK_REALM=chexpro

# Backend Client ID (from Keycloak admin console)
KEYCLOAK_CLIENT_ID=chexpro-backend

# Backend Client Secret (from Keycloak → Clients → chexpro-backend → Credentials)
KEYCLOAK_CLIENT_SECRET=your-client-secret-here

# Expected audience in tokens
KEYCLOAK_AUDIENCE=chexpro-backend

# Token validation
KEYCLOAK_TOKEN_EXPIRATION=300
KEYCLOAK_CLOCK_SKEW=30

# Production URLs (comment out for development)
# KEYCLOAK_URL=https://auth.chexpro.com
# KEYCLOAK_REALM=chexpro
# KEYCLOAK_CLIENT_ID=chexpro-backend
# KEYCLOAK_CLIENT_SECRET=your-production-client-secret
# KEYCLOAK_AUDIENCE=chexpro-backend
