# Keycloak Authentication Configuration
# Copy this file to .env and fill in the values

# Keycloak Server URL
VITE_KEYCLOAK_URL=http://localhost:8081

# Keycloak Realm
VITE_KEYCLOAK_REALM=chexpro

# Frontend Client ID (from Keycloak admin console)
VITE_KEYCLOAK_CLIENT_ID=chexpro-frontend

# Application URL
VITE_APP_URL=http://localhost:5173

# Production URLs (comment out for development)
# VITE_KEYCLOAK_URL=https://auth.chexpro.com
# VITE_KEYCLOAK_REALM=chexpro
# VITE_KEYCLOAK_CLIENT_ID=chexpro-frontend
# VITE_APP_URL=https://chexpro.com
